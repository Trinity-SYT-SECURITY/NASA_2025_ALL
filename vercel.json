{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "installCommand": "npm install",
    "framework": "create-react-app",
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          }
        ]
      }
    ]
  }
  